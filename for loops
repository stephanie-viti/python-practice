for x in range(5):
    print(x)
#output
#0
#1
#2
#3
#4

friends = ['Taylor', 'Alex', 'Pat', 'Eli']
for friend in friends:
    print("Hi " + friend)
#output
#Hi Taylor
#Hi Alex
#Hi Pat
#Hi Eli

values = [ 23, 52, 59, 37, 48 ]
sum = 0
length = 0
for value in values:
    sum += value
    length += 1
#output: Total sum: 219 - Average: 43.8

print("Total sum: " + str(sum) + " - Average: " + str(sum/length)
